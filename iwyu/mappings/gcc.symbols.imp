  { "symbol": [ "aiocb", "private", "<aio.h>", "private"] },
  { "symbol": [ "blkcnt_t", "private", "<sys/types.h>", "private"] },
  { "symbol": [ "blksize_t", "private", "<sys/types.h>", "private"] },
